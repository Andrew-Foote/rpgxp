echo "Typechecking the codebase..."
typecheck
echo "Generating classes..."
env/bin/python -m rpgxp.generate_classes
echo "Generating database schema..."
env/bin/python -m rpgxp.generate_db_schema
echo "Generating database data..."
env/bin/python -m rpgxp.generate_db_data ${1+"$1"}
echo "Checking foreign keys..."
env/bin/python -m rpgxp.script.foreign_key_report